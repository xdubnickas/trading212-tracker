{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://live.trading212.com/api/v0/:path*"
    },
    {
      "source": "/csv-proxy/:path*",
      "destination": "https://tzswiy3zk5dms05cfeo.s3.eu-central-1.amazonaws.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
        }
      ]
    }
  ]
}
